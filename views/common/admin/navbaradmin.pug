mixin navbar(user)
  nav.navbar.navbar-expand.navbar-dark.bg-dark
    a(href="/", aria-label="Golden Goose").navbar-brand.mr-0.mr-md-2
      img(src="/images/1.jpg", width="36", height="36")
    .collapse.navbar-collapse
      ul.navbar-nav.mr-auto
        li.nav-item
          a.nav-link(href="/admin_home") Home
        li.nav-item
          a.nav-link(href="/admin_signup") add for admin
    .navbar-right
      ul.navbar-nav.mr-auto
        li.dropdown.user.user-menu
          a.dropdown-toggle(href='#' data-toggle='dropdown' aria-expanded='false')
            img.user-image(src='/AdminLTE-3.1.0-rc/dist/img/user2-160x160.jpg' alt='User Image')
            span.hidden-xs admin
          ul.dropdown-menu
            li.user-header
              img.img-circle(src='/AdminLTE-3.1.0-rc/dist/img/user2-160x160.jpg' alt='User Image')
              p
              | admin - admin
              small Web appication
            li.user-body
              .row
                .col-4.text-center
                   p Followers
                .col-4.text-center
                   p Sales
                .col-4.text-center
                   p Friends
            li.user-footer
              .pull-right
                a.btn.btn-default.btn-flat(href='#') Profile
              .pull-right
                a.btn.btn-default.btn-flat(href='#') Sign out
        li.nav-item
          a.nav-link(href="/admin_login") Login